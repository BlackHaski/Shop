<div class="width100p height-auto">
    <img id="avatar" class="float-l" src="${user.userInfo.img}" height="400px" width="30%">
    <div class="width65p float-r">
        <h1 id="username" class="color-green">${user.username}</h1>

        <h1 class="float-l font-size-20px color-gray">Email:&emsp;</h1>
        <h1 id="email" class="font-size-20px float-l color-white">${user.email}</h1>

        <h1 class="float-l font-size-20px clear-b color-gray">Birthday:&emsp;</h1>
        <h1 id="birthday" class="font-size-20px float-l color-white">${user.userInfo.birthday}</h1>
    </div>
</div>